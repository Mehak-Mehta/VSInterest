{
	"name": "vsinterest",
	"displayName": "VSinterest",
	"description": "Finding interest",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:VSInterest-sidebar",
		"onCommand:vsinterest.helloWorld"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "VSInterest-sidebar-view",
					"title": "VSInterest",
					"icon": "media/breakpoint.svg"
				}
			]
		},
		"views": {
			"VSInterest-sidebar-view": [
				{
					"type": "webview",
					"id": "VSInterest-sidebar",
					"name": "VSInterest",
					"icon": "media/breakpoint.svg",
					"contextualTitle": "VSInterest"
				}
			]
		},
		"commands": [
			{
				"command": "vsinterest.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "concurrently \"rollup -c -w\" \"webpack --watch ",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^19.0.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"@rollup/plugin-typescript": "^8.2.1",
		"@tsconfig/svelte": "^1.0.10",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"concurrently": "^6.1.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"postcss": "^8.3.0",
		"rollup": "^2.3.4",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-postcss": "^4.0.0",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript": "^1.0.1",
		"rollup-watch": "^4.3.1",
		"svelte": "^3.38.2",
		"svelte-check": "^1.5.4",
		"svelte-preprocess": "^4.7.3",
		"ts-loader": "^8.0.14",
		"typescript": "^4.2.4",
		"typings": "^2.1.1",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"@sveltejs/svelte-virtual-list": "^3.0.1"
	}
}
